<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="">
    <title>@ViewBag.Title</title>
    <!-- Favicons-->
    <link rel="apple-touch-icon-precomposed" sizes="57x57" href="~/Content/images/apple-touch-icon-57x57.png" />
    <link rel="apple-touch-icon-precomposed" sizes="114x114" href="~/Content/images/apple-touch-icon-114x114.png" />
    <link rel="apple-touch-icon-precomposed" sizes="76x76" href="~/Content/images/apple-touch-icon-76x76.png" />
    <link rel="icon" type="image/png" href="~/Content/images/favicon-32x32.png" sizes="32x32" />

    <!-- Google Web Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Lato:100,100i,300,300i,400,400i,700,700i,900,900i" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Roboto:100,100i,300,300i,400,400i,500,500i,700,700i,900,900i" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Roboto+Condensed:300,300i,400,400i,700,700i" rel="stylesheet">

    <!-- Global CSS -->
    @Styles.Render("~/Content/css")
</head>
<body>
    <!-- Header -->
    <header class="header-wrap">
        <div class="header-home">
            <!-- Header and Navigation -->
            @Html.Partial("_TopNavBar")
        </div>
        <!-- Page specific header content-->
        @RenderSection("header_content", required: true)
    </header>

    <!-- Body -->
    @RenderBody()

    <!-- Footer -->
    @Html.Partial("_Footer")

    <!-- Scripts -->
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/bundles/popper")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/magnific-popup")
    @Scripts.Render("~/bundles/owl-carousel")
    @Scripts.Render("~/bundles/rangeslider")
    <!-- Google Map -->
    @RenderSection("map_script", required: false)
    @Scripts.Render("~/bundles/scripts")

    <!-- View specific scripts -->
    @RenderSection("scripts", required: false)
</body>
</html>